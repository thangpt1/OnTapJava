<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
    <appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>hw.log</file>
        <encoder>
            <pattern>%date %msg%n</pattern>
            <!--<pattern>%date %level [%thread] %logger{10} [%file:%line] %msg%n</pattern>-->
        </encoder>
    </appender>
    <root level="debug">
        <appender-ref ref="FILE" />
    </root>
</Configuration>
